<?xml version="1.0" encoding="ISO-8859-1" ?>

<taglib>

   <!-- ============== Tag Library Description Elements ============= -->

   <tlibversion>1.0</tlibversion>
   <jagversion>1.0</jagversion>
   <shortname>jag</shortname>

   <info></info>

   <!-- ===================== Control Flow Tags ====================== -->
   <tag>
      <name>write</name>
      <tagclass>com.finalist.jag.taglib.WriteTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>display</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>pathenable</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>iterator</name>
      <tagclass>com.finalist.jag.taglib.IteratorTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>id</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>seperator</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>max</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>counter</name>
         <required>false</required>
      </attribute>
   </tag>


   <tag>
      <name>file</name>
      <tagclass>com.finalist.jag.taglib.FileTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>path</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>title</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>ext</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>name</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
   </tag>


   <tag>
      <name>equal</name>
      <tagclass>com.finalist.jag.taglib.EqualTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>sensitive</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>parameter</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>variable</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>notEqual</name>
      <tagclass>com.finalist.jag.taglib.NotEqualTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>sensitive</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>parameter</name>
         <required>false</required>
      </attribute>
      <attribute>
         <name>variable</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>exception</name>
      <tagclass>com.finalist.jag.taglib.ExceptionTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>message</name>
         <required>true</required>
      </attribute>
   </tag>

   <tag>
      <name>clearVariable</name>
      <tagclass>com.finalist.jag.taglib.ClearVariableTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
   </tag>

   <tag>
      <name>exist</name>
      <tagclass>com.finalist.jag.taglib.ExistTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>notExist</name>
      <tagclass>com.finalist.jag.taglib.NotExistTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>counter</name>
      <tagclass>com.finalist.jag.taglib.CounterTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>odd</name>
         <required>true</required>
      </attribute>
   </tag>

   <tag>
      <name>empty</name>
      <tagclass>com.finalist.jag.taglib.EmptyTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>notEmpty</name>
      <tagclass>com.finalist.jag.taglib.NotEmptyTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>property</name>
         <required>false</required>
      </attribute>
   </tag>

   <tag>
      <name>variable</name>
      <tagclass>com.finalist.jag.taglib.VariableTag</tagclass>
      <bodycontent>java</bodycontent>
      <attribute>
         <name>name</name>
         <required>true</required>
      </attribute>
      <attribute>
         <name>list</name>
         <required>false</required>
      </attribute>
   </tag>
</taglib>
